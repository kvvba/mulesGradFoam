FoamFile
{
  version     2.0;
  format      ascii;
  class       volScalarField;
  object      TL;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField uniform 373.15;

