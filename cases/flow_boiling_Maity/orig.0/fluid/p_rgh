FoamFile
{
    version 2.0;
    format ascii;
    class volScalarField;
    object p_rgh;
}

dimensions [1 -1 -2 0 0 0 0];

internalField uniform 1.0e5;

boundaryField
{
    inlet
    {
        type fixedFluxPressure;
    }
    outlet
    {
        // type totalPressure;
        // p0 uniform 0;
        // value uniform 0;
        type zeroGradient;
    }
    top
    {
        type fixedValue;
        value uniform 1.0e5;
        // type fixedFluxPressure;
    }
    silicon
    {
        type fixedFluxPressure;
    }
    front
    {
        type symmetryPlane;
    }
    back
    {
        // type totalPressure;
        // p0 uniform 0;
        // value uniform 0;
        type zeroGradient;
        // type fixedFluxPressure;
    }
}

/// p_rgh ends here
